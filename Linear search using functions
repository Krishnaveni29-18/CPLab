#include<stdio.h>
int l_search(int arr[],int size)
{
  int ket,i;
  printf("enter key value:");
  scanf("%d",&key);
  for(i=0;i<size;i++)
    {
      if(key==arr[i])
         return i;
    }
  }
return -1;
int main(){
  int arr[]={1,2,3,4,5}},result;
  resul=l_search(arr,5);
  if(result!=-1)
    printf("the element is found at %d index",result);
  else
     printf("the element is not found");
}
